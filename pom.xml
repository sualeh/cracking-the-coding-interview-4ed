  <name>cracking-the-coding-interview-4ed</name>
  <url>http://maven.apache.org</url>
  <licenses>
    <license>
      <name>GNU Lesser General Public License</name>
      <url>http://www.gnu.org/copyleft/lesser.txt</url>
    </license>
  </licenses>
  <developers>
